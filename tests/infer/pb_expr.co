// RUN: choreo -i %s | FileCheck --match-full-lines %s

__co__ auto foo(int a, int b) {
  parallel p by a + 3, q = {qx, qy} by [7 * 5 - 2, 6 * b - 3] {}
}

// CHECK: Bounded:   ::foo{{(::paraby_[0-9]+)*}}::p, Type: {int}->[(::foo::a + 3)]
// CHECK: Bounded:   ::foo{{(::paraby_[0-9]+)*}}::p__elem__x, Type: {int}->[(::foo::a + 3)](pi:x)
// CHECK: Bounded:   ::foo{{(::paraby_[0-9]+)*}}{{(::paraby_[1-9]+)*}}::q, Type: {int,int}->[33, ((::foo::b * 6) - 3)]
// CHECK: Bounded:   ::foo{{(::paraby_[0-9]+)*}}{{(::paraby_[1-9]+)*}}::qx, Type: {int}->[33](pi:x)
// CHECK: Bounded:   ::foo{{(::paraby_[0-9]+)*}}{{(::paraby_[1-9]+)*}}::qy, Type: {int}->[((::foo::b * 6) - 3)](pi:y)
