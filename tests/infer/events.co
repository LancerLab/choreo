// RUN: choreo -i %s | FileCheck --match-full-lines %s

__co__ void foo() {
  global event a;
  global event aa[3];
  parallel by 3 {
    shared event b;
    shared event bb[7];
    parallel by 2 {
      local event c;
      local event cc[9];
    }
  }
}

// CHECK: Symbol:    ::foo::a, Type: global event
// CHECK: Symbol:    ::foo::aa, Type: global event{{\[\[3\]\]}}
// CHECK: Symbol:    ::foo{{(::paraby_[0-9]+)*}}::b, Type: shared event
// CHECK: Symbol:    ::foo{{(::paraby_[0-9]+)*}}::bb, Type: shared event{{\[\[7\]\]}}
// CHECK: Symbol:    ::foo{{(::paraby_[0-9]+)*}}::c, Type: local event
// CHECK: Symbol:    ::foo{{(::paraby_[0-9]+)*}}::cc, Type: local event{{\[\[9\]\]}}
