// RUN: choreo --debug-visit=valno -sa=check %s | FileCheck %s

__co__ void add() {
   a = 1 + 2;
   b = 1;
   c = a + b;
}

// CHECK: <symval> ::add::a: 3
// CHECK: <symval> ::add::b: 1
// CHECK: <symval> ::add::c: 4

__co__ void sym_add(int b) {
   a = 1 * 2 - b;
   c = a + b;
}

// CHECK: <symval> ::sym_add::a: (2 - ::sym_add::b)
// CHECK: <symval> ::sym_add::c: 2
