// RUN: choreo -e %s | FileCheck --match-full-lines %s

// device program
__co__ auto foo() {
  int x = -0x7fffffff;
  y = 0x01;
  int z = -0x7FFFFFFF - 1;
  s32 [0x10, 0x100] g;
  mds : [0xf, 0x1f];
  foreach {p, q} in [0xa, 0xaa] {}
  return 0xFF;
}

// CHECK: ChoreoFunction
// CHECK:  `- Name: foo
// CHECK:  `- Return type: unknown
// CHECK:  `- Parameters:
