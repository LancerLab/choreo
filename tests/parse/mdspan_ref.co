// RUN: choreo -e %s | FileCheck --match-full-lines %s

__co__ int span_element(int d, int c) {
  mdspan dims : [9, d];  // shape, mdspan node
  dim1 : [c, 10];
  int a = dims(c) + dim1(0) + d;
}

// CHECK: ChoreoFunction
// CHECK:  `- Name: span_element
// CHECK:  `- Return type: s32
// CHECK:  `- Parameters:
// CHECK:    `-  type: s32, symbol: d
// CHECK:    `-  type: s32, symbol: c
// CHECK:  `- Type Decl: dims - [ 9 d ]
// CHECK:  `- Type Decl: dim1 - [ c 10 ]
// CHECK:  `- Var Decl (s32): a =  ( (dims(c) + dim1(0))  + d)
